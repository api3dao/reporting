{
  "name": "beacon-reporter",
  "version": "0.1.0",
  "description": "A tool to analyse beacon activity data",
  "main": "index.js",
  "engine": "^14.17.1",
  "scripts": {
    "report": "yarn ts-node src/main.ts",
    "gas-all": "yarn report --query=beacons-gas-cost-all --output=html",
    "gas-all-1month": "yarn report --query=beacons-gas-cost-interval --time=m --interval=1 --output=html",
    "gas-all-6month": "yarn report --query=beacons-gas-cost-interval --time=m --interval=6 --output=html",
    "gas-all-1week": "yarn report --query=beacons-gas-cost-interval --time=w --interval=1 --output=html",
    "gas-all-1year": "yarn report --query=beacons-gas-cost-interval --time=y --interval=1 --output=html",
    "gas-polygon": "yarn report --query=beacon-gas-cost --chain=polygon --output=html",
    "gas-polygon-testnet": "yarn report --query=beacon-gas-cost --chain=polygon-testnet --output=html",
    "prettier:check": "prettier --check \"./**/*.{js,sol,ts,md,yml,json}\"",
    "prettier:write": "prettier --write \"./**/*.{js,sol,ts,md,yml,json}\" --loglevel silent",
    "lint": "yarn run lint:eslint && yarn prettier:check",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:eslint:fix": "eslint . --ext .js,.ts --fix",
    "clean": "rimraf -rf ./dist *.tgz",
    "compile": "tsc -p tsconfig.json",
    "build": "yarn run clean && yarn run compile",
    "docker": "docker build -t api3/reporting:latest -f docker/Dockerfile ."
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "@types/pg-format": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@api3/airnode-protocol-v1": "^0.4.0",
    "@api3/operations": "latest",
    "dotenv": "^10.0.0",
    "ethers": "^5.6.8",
    "minimist": "^1.2.6",
    "pg": "^8.7.1",
    "pg-format": "^1.0.4",
    "source-map-support": "^0.5.21"
  }
}
